{
  "name": "oga-vscode",
  "displayName": "oga-vscode",
  "description": "VS Code extension for the Oga programming language",
  "version": "0.0.1",
  "publisher": "tobiade",
  "icon": "./images/oga-at-the-top.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tobiade/oga-vscode.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "oga",
        "aliases": [
          "Oga",
          "oga"
        ],
        "extensions": [
          ".og"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/coat-of-arms.png",
          "dark": "./images/coat-of-arms.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "oga",
        "scopeName": "source.oga",
        "path": "./syntaxes/oga.tmLanguage.json"
      }
    ]
  }
}